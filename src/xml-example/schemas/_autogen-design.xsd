    <xs:complexType name="_Notes--Boxed__Body"><xs:attribute name="font-family" use="required" type="xs:string"><xs:annotation><xs:documentation>required. The value needs to be a os:font</xs:documentation></xs:annotation></xs:attribute><xs:attribute name="background-color" use="required" type="xs:string"><xs:annotation><xs:documentation>required. The value needs to be a os:color</xs:documentation></xs:annotation></xs:attribute></xs:complexType>
    
